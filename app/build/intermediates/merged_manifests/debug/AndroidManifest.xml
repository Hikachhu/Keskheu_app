<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.keskheu"
    android:versionCode="3"
    android:versionName="3.1.2" >

    <uses-sdk
        android:minSdkVersion="21"
        android:targetSdkVersion="30" />

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:appComponentFactory="androidx.core.app.CoreComponentFactory"
        android:debuggable="true"
        android:icon="@mipmap/ic_launcher_keskeu"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round_keskeu"
        android:supportsRtl="true"
        android:theme="@style/Theme.Keskheu"
        android:usesCleartextTraffic="true" >
        <activity
            android:name="com.example.keskheu.MainActivity"
            android:label="@string/app_name"
            android:theme="@style/Theme.Keskheu.NoActionBar" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>